<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:context="http://www.springframework.org/schema/context"
  xmlns:p="http://www.springframework.org/schema/p"
  xsi:schemaLocation="
		http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
		http://www.springframework.org/schema/context
		http://www.springframework.org/schema/context/spring-context-3.2.xsd">

  <!-- demo start -->
  <bean id="startExecution"
    class="com.github.processx.api.demo.StartExecution"/>

  <bean id="accessExecution"
    class="com.github.processx.api.demo.AccessExecution"/>

  <bean id="preCreditExecution"
    class="com.github.processx.api.demo.PreCreditExecution"/>

  <bean id="preCreditQueryExecution"
    class="com.github.processx.api.demo.PreCreditQueryExecution"/>

  <bean id="bindingCardExecution"
    class="com.github.processx.api.demo.BindingCardExecution"/>

  <bean id="creditExecution"
    class="com.github.processx.api.demo.CreditExecution"/>

  <bean id="creditQueryExecution"
    class="com.github.processx.api.demo.CreditQueryExecution"/>

  <bean id="signExecution"
    class="com.github.processx.api.demo.SignExecution"/>

  <bean id="endExecution"
    class="com.github.processx.api.demo.EndExecution"/>
  <!-- demo end -->

</beans>

